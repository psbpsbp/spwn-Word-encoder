return {
    word_to_list: (word,list) {
        let letters = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz".split("");let list = [];let word = word.split(" ")
        for words in 0..word.length {
        list.push([])
        for letter in 0..word[words].length {
            list[words].push(letters.index(word[words][letter])+1)
            }
        }
        return list
    }
}
